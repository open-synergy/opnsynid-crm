<odoo>
<data noupdate="1">
    <record id="crm_funnel_type_rule" model="ir.rule">
        <field name="model_id" ref="crm.model_crm_lead" />
        <field
                name="domain_force"
            >[('funnel_type_id.member_ids','in',[user.id])]</field>
        <field name="name">Funnel Type Rule for All Sales/User</field>
        <field
                name="groups"
                eval="[(4, ref('crm_lead_funnel_type.crm_funnel_type_allowed_group'))]"
            />
    </record>
</data>
</odoo>
